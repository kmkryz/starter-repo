<div class="management-container">
    <h1>Account Management</h1>
    <% if (messages && messages.length) { %>
      <div class="flash-messages">
        <% messages.forEach(function(message) { %>
          <p><%= message %></p>
        <% }); %>
      </div>
    <% } %>
    <p>You're logged in.</p>
  </div>
  